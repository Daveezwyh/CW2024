<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.layout.HBox?>
<?import javafx.geometry.Insets?>

<StackPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
           fx:controller="com.example.demo.controller.MainMenuController">
    <ImageView fx:id="backgroundImageView" fitWidth="1300" fitHeight="750" />

    <HBox alignment="CENTER" prefHeight="61.0" prefWidth="1300.0" spacing="10">
        <children>
            <Button onAction="#onStartGame" prefWidth="150.0" text="Start">
                <HBox.margin>
                    <Insets left="10.0" right="10.0" />
                </HBox.margin>
            </Button>
            <Button onAction="#onExit" prefWidth="150.0" text="Exit">
                <HBox.margin>
                    <Insets left="10.0" right="10.0" />
                </HBox.margin>
            </Button>
        </children>
    </HBox>
</StackPane>
